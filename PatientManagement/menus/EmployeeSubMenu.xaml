<UserControl x:Class="PatientManagement.menus.EmployeeSubMenu"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Border BorderBrush="Black" BorderThickness="0 0 2 0">
            <Grid Margin="5,0,5,0">
                <StackPanel>
                    <Button Name="Home" Content="Home" Margin="0,5,0,5" FontSize="12pt"></Button>
                    <Button Name="Patients" Content="Patients" Margin="0,5,0,5" FontSize="12pt"></Button>
                    <Button Name="Doctors" Content="Doctors" Margin="0,5,0,5" FontSize="12pt"></Button>
                    <Button Name="Appointments" Content="Appointments" Margin="0,5,0,5" FontSize="12pt"></Button>
                    <Button Name="Logout" Content="Logout" Margin="0,5,0,5" FontSize="12pt"></Button>
                </StackPanel>
            </Grid>
        </Border>
    </Grid>
</UserControl>
